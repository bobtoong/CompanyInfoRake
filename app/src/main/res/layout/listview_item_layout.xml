<?xml version="1.0" encoding="utf-8"?>
<com.bobtoong.companyinforake.CheckableLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:id="@+id/customListViewcheckBox"
        android:focusable="false"
        android:clickable="false"
        android:layout_weight="1" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:id="@+id/customListViewtextView"
        android:textSize="20sp"
        android:gravity="left"
        android:layout_weight="5" />
</com.bobtoong.companyinforake.CheckableLinearLayout>
